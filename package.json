{
  "name": "kelly-hayn-resumes",
  "version": "1.0.0",
  "description": "My resume(s) as JSON files, to be built by resume-cli.",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf docs",
    "predeploy": "git checkout gh-pages && git merge master && npm run build && git add -A docs && git commit -m \"Update resume\" || echo \"No changes to docs. Proceeding with git push.\"",
    "deploy": "git push origin gh-pages",
    "postdeploy": "git checkout master",
    "prebuild": "npm run clean",
    "build": "node .",
    "postbuild": "git restore docs/CNAME",
    "eslint": "eslint",
    "nodemon": "nodemon",
    "resume": "resume",
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "nodemon --watch src ."
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com/davidlwatsonjr/resume.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://gitlab.com/davidlwatsonjr/resume/issues"
  },
  "homepage": "https://gitlab.com/davidlwatsonjr/resume#README",
  "dependencies": {
    "glob": "^7.1.2",
    "jsonresume-theme-elegant": "^1.16.1",
    "jsonresume-theme-short": "^0.1.7",
    "resume-cli": "^0.4.19"
  },
  "devDependencies": {
    "eslint": "^9.10.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "nodemon": "^3.1.4",
    "prettier": "^3.3.3",
    "rimraf": "^2.6.2"
  }
}
